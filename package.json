{
  "name": "karma-test-skeleton",
  "version": "0.0.1",
  "description": "Karma Test skeleton code base to run js tests in multiple browsers",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "karma start test/conf/karma-dev.js --single-run --browsers PhantomJS",
    "test-dev": "karma start test/conf/karma-dev.js",
    "test-all": "karma start test/conf/karma-all.js",
    "test-ci": "karma start test/conf/karma-ci.js",
    "test-debug": "karma start test/conf/karma-debug.js"
  },
  "author": "Lawrence Carvalho",
  "license": "MIT",
  "devDependencies": {
    "mocha": "~1.15.1",
    "chai": "~1.8.1",
    "sinon": "~1.7.3",
    "sinon-chai": "~2.4.0",
    "karma": "~0.10.7",
    "iectrl": "0.0.3",
    "karma-mocha": "~0.1.1",
    "karma-chai-sinon": "~0.1.0",
    "karma-ievms": "0.0.4",
    "karma-ie-launcher": "~0.1.1",
    "karma-script-launcher": "~0.1.0",
    "karma-chrome-launcher": "~0.1.1",
    "karma-firefox-launcher": "~0.1.2",
    "karma-phantomjs-launcher": "~0.1.1",
    "karma-coverage": "~0.1.3",
    "karma-html2js-preprocessor": "~0.1.0",
    "karma-junit-reporter": "~0.2.1",
    "karma-htmlfile-reporter": "~0.1.0"
  }
}
